.wrapperContact
  background: #ffffff

.block-contactHeader
  background: url("/images/contact-bg.jpg") 0 0 no-repeat
  background-size: cover
  width: 100%
  height: 400px

.block-contactMenu
  width: 769px
  margin: 0 auto
  text-align: center

.block-contactMenu_elem-name
  margin: 90px auto

.block-contactMenu_elem-name1
  font-size: 10pt
  color: #d4af37
  font-weight: 600

.block-contactMenu_elem-name2
  font-size: 38pt
  color: #021725
  font-weight: 600
  opacity: 0.6

.block-contactMenu_elem-container
  width: auto

.block-contactMenu_elem-cd1
  background: #d4af37
  height: 300px
  width: 300px
  display: inline-block
  float: left
  text-align: left

.block-contactMenu_elem-cd1_elem-txt
  color: #ffffff
  font-size: 17pt
  display: inline-block
  width: 185px
  margin: 11px 32px

.block-contactMenu_elem-line
  border: 2px solid #ffffff
  color: #ffffff
  width: 32px
  margin: 32px

.block-contactMenu_elem-img
  display: inline-block
  float: right

.block-contactMenu_elem-cd2
    background: #000000
    height: 300px
    width: 300px
    display: inline-block
    float: right
    text-align: left
    margin-left: 5px

.block-contactMenu_elem-cd2_elem-txt
  color: #ffffff
  font-size: 15pt
  display: inline-block
  margin-left: 32px

.block-contactMenu_elem-cd3
  background: #f9f9f9
  text-align: left
  display: inline-block
  float: left
  width: 60%
  height: 300px

.block-contactMenu_elem-cd3_elem-txt
  color: #3f3f3f
  font-size: 15pt
  display: inline-block
  padding: 47px

.block-contactButton
  background: #ffffff
  margin-top: 77px
  margin-bottom: 89px
